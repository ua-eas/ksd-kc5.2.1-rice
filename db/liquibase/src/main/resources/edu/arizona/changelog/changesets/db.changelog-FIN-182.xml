    <changeSet id="FIN-1053_Parameter_Permission_Cleanup" author="Julie Wingate">
        <comment>
            Remaining Table/Parameter/Permission Cleanup
        </comment>
        <sql>
            DELETE FROM RICE.KRCR_PARM_T WHERE NMSPC_CD IN ('KFS-AR','KFS-CAM','KFS-CG','KFS-COA','KFS-CR','KFS-EC','KFS-FP','KFS-GL','KFS-LD','KFS-PACS','KFS-PCARD','KFS-PDP','KFS-PRJE','KFS-PURAP','KFS-SEC','KFS-TAX','KFS-VND');
